import numpy as np # linear algebra
import pandas as pd # data processing, CSV file I/O (e.g. pd.read_csv)
from sklearn.model_selection import train_test_split
from sklearn.naive_bayes import MultinomialNB
from sklearn.svm import SVC
from sklearn.ensemble import RandomForestClassifier
from sklearn.metrics import accuracy_score

df1 = pd.read_csv("C:/Users/Atif/Documents/Atif/1 MCS-KIET/4-Spring Semester 2022/109214-Artificial Intelligence and Expert Systems/Project/tabular-playground-series-may-2022/test.csv")
print("---Original Train Data---")
pd.set_option('display.max_columns', None)
print(df1.iloc[:,1:32])
# Dropping string column f27
df = df1.drop(['f_27'], axis = 1)
print(df.head(10))
# apply the dtype attribute
result = df.dtypes
print("---Data Types---")
print(result)
df[df < 0] = 0
print("---Data after cleaning---")
print(df)
print("Data Information")
print( df.info())
print("Null Data Information")
print(df.isnull().sum())
print("Describing Data")
print(df.describe())
print("Data Correlation")
print(df.corr())

# Creating the Naive Bayes Model
X = df.iloc[:,1:700000]
print("Printing X values")
print(X)
Y = df.iloc[:,-1].values
print("Printing Y values")
print(Y)

train_x,test_x,train_y,test_y = train_test_split(X,Y,test_size = 0.30)
# Naive Bayes
# For Laplace smoothing setting alpha=1
mnb = MultinomialNB(alpha=1)
# alpha= 1 in Laplace and < 1 in Lidstone Smoothing
mnb.fit(train_x,train_y)
y_pred1 = mnb.predict(test_x)
print("Accuracy Score for Naive Bayes - Laplace Smoothing : ", accuracy_score(y_pred1,test_y))

